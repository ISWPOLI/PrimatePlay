# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index()
GET     /home                       controllers.HomeController.home()
GET     /catalog                       controllers.HomeController.catalog()
#POST    /person                     controllers.HomeController.addPerson()
GET     /persons                    controllers.HomeController.getPersons()

GET     /user                       controllers.UserController.load()
POST    /userpost                   controllers.UserController.addUser()
#POST    /authenticate               controllers.UserController.authenticate()
GET     /users                      controllers.UserController.getUsers()

GET     /product                       controllers.ProductController.loadProduct()
POST    /productpost                   controllers.ProductController.addProduct()
GET     /products                     controllers.ProductController.getProducts()


POST  /login                         controllers.LoginController.validate()

# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /car                        controllers.Assets.at(path="/public", file="/images/car.png")
GET     /1                        controllers.Assets.at(path="/public", file="/images/1.jpg")
GET     /2                        controllers.Assets.at(path="/public", file="/images/2.jpg")
GET     /3                        controllers.Assets.at(path="/public", file="/images/3.jpg")
GET     /4                        controllers.Assets.at(path="/public", file="/images/4.jpg")
GET     /5                        controllers.Assets.at(path="/public", file="/images/5.jpg")
GET     /6                        controllers.Assets.at(path="/public", file="/images/6.jpg")
GET     /7                        controllers.Assets.at(path="/public", file="/images/7.jpg")
GET     /8                        controllers.Assets.at(path="/public", file="/images/8.jpg")
GET     /9                        controllers.Assets.at(path="/public", file="/images/9.jpg")
GET     /10                        controllers.Assets.at(path="/public", file="/images/10.jpg")
